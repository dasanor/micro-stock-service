{
  "services": {
    "name": "stock",
    "version": "v1",
    "style": "RPC"
  },
  "models": {
    "Stock": "./models/stockModel",
    "Reserve": "./models/reserveModel"
  },
  "schemas": {
    "renewReserve": "./schemas/renewReserveSchema"
  },
  "reserveChain": {
    "getStock": "./operations/chains/reserve/getStock",
    "preReserveStock": "./operations/chains/reserve/preReserveStock",
    "reserveStock": "./operations/chains/reserve/reserveStock"
  },
  "unreserveChain": {
    "getReserve": "./operations/chains/unreserve/getReserve",
    "preUnreserveStock": "./operations/chains/unreserve/preUnreserveStock",
    "unreserveStock": "./operations/chains/unreserve/unreserveStock"
  },
  "workers": [
    {
      "worker": "unreserveExpired",
      "handler": "./jobs/unreserveExpired",
      "when": "0 */1 * * * *"
    }
  ],
  "defaultwarehouseId": "001",
  "minQuantity": 1,
  "minutesToReserve": 20,
  "allowReserveTimeOverwrite": false,
  "reserveActive": true
}
